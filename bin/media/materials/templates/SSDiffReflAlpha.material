			{
			}
			fragment_program_ref rex/DiffReflAlphaFP
			{
				param_named opacity       float 0.5
			}
			texture_unit baseMap
			{
				texture TextureMissing.png
			}
			texture_unit reflectionMap
			{
				texture TextureMissing.png
			}
		}
	}
}
